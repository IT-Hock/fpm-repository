<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="{% if page contains "description" %}{{ page.description }}{% else %}{{ site.description }}{% endif %}">
    <meta name="keywords" content="{% if page contains "keywords" %}{{ page.keywords }},{% endif %}{{ site.keywords }}">
    <meta name="author" content="{% if page contains "author" %}{{ page.author }}{% else %}{{ site.author }}{% endif %}">
    <meta name="generator" content="Jekyll v{{ jekyll.version }}">
    <title>{% if page contains "title" %}{{ page.title }} :: {% endif %}{{ site.title }}</title>

    <link rel="shortcut icon" href="{{ "/assets/img/favicon.ico" | prepend: site.baseurl_root }}">
    <link rel="icon" type="image/x-icon" href="{{ "/assets/img/favicon.ico" | prepend: site.baseurl_root }}">
    <link rel="icon" type="image/png" href="{{ "/assets/img/favicon.png" | prepend: site.baseurl_root }}">

    <link rel="apple-touch-icon" sizes="57x57" href="{{ "/assets/img/apple/touch-icon-57x57.png" | prepend: site.baseurl_root }}">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ "/assets/img/apple/touch-icon-72x72.png" | prepend: site.baseurl_root }}">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ "/assets/img/apple/touch-icon-114x114.png" | prepend: site.baseurl_root }}">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ "/assets/img/apple/touch-icon-144x144.png" | prepend: site.baseurl_root }}">

    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{{ "/assets/img/mstile/144x144.png" | prepend: site.baseurl_root }}">

    <meta name="theme-color" content="#ffffff">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="application-name" content="{{ site.title }}">
    <link rel="icon" sizes="48x48" href="{{ "/assets/img/android/chrome-48x48.png" | prepend: site.baseurl_root }}">
    <link rel="icon" sizes="72x72" href="{{ "/assets/img/android/chrome-72x72.png" | prepend: site.baseurl_root }}">
    <link rel="icon" sizes="96x96" href="{{ "/assets/img/android/chrome-96x96.png" | prepend: site.baseurl_root }}">
    <link rel="icon" sizes="144x144" href="{{ "/assets/img/android/chrome-144x144.png" | prepend: site.baseurl_root }}">
    <link rel="icon" sizes="192x192" href="{{ "/assets/img/android/chrome-192x192.png" | prepend: site.baseurl_root }}">


    <title>{% if page contains "title" %}{{ page.title }} | {% endif %}{{ site.title }}</title>
    <script src="{{ "/assets/dist/main.js" | prepend: site.baseurl_root }}?v={{ site.git.head.short_id }}"></script>

    {% if page contains "stylesheets" %}
        {% for stylesheet in page.stylesheets %}
            <link rel="stylesheet" href="{{ stylesheet }}" />
        {% endfor %}
    {% endif %}
    {% if page contains "scripts" %}
        {% for script in page.scripts %}
            <script src="{{ script | prepend: "/assets/js/" | prepend: site.baseurl }}?v={{ site.git.head.short_id }}"></script>
        {% endfor %}
    {% endif %}

    <script type="application/javascript">
        app.$(function () {
            app.init();
        });
    </script>
</head>